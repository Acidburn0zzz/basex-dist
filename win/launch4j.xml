<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>BaseX.jar</jar>
  <outfile>BaseX.exe</outfile>
  <errTitle></errTitle>
  <cmdLine></cmdLine>
  <chdir></chdir>
  <priority>normal</priority>
  <downloadUrl></downloadUrl>
  <supportUrl></supportUrl>
  <customProcName>false</customProcName>
  <stayAlive>false</stayAlive>
  <manifest></manifest>
  <icon>../images/BaseX.ico</icon>
   <classPath>
    <mainClass>org.basex.BaseXGUI</mainClass>
    <cp>.\lib\tagsoup-1.2.jar</cp>
    <cp>.\lib\lucene-analyzers-3.0.2.jar</cp>
    <cp>.\lib\snowball.jar</cp>
  </classPath>
  <jre>
    <path></path>
    <minVersion>1.6.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <maxHeapSize>1024</maxHeapSize>
  </jre>
  <versionInfo>
    <fileVersion>$f</fileVersion>
    <txtFileVersion>BaseX $v</txtFileVersion>
    <fileDescription>Native XML Database and XQuery Processor</fileDescription>
    <copyright>(c) BaseX Team, 2011</copyright>
    <productVersion>$f</productVersion>
    <txtProductVersion>BaseX $v</txtProductVersion>
    <productName>BaseX GUI</productName>
    <companyName>BaseX Team</companyName>
    <internalName>BaseX GUI</internalName>
    <originalFilename>BaseX.exe</originalFilename>
  </versionInfo>
</launch4jConfig>